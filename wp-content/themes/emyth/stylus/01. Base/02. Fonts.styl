// Titillium
@font-face
	font-family 'Titillium Web'
	font-style normal
	font-weight 400
	src url('fonts/Titillium_Web.eot')
	src url('fonts/Titillium_Web.eot?#iefix') format('embedded-opentype'),
		url('fonts/Titillium_Web.svg#TitilliumWeb') format('svg'),
		url('fonts/Titillium_Web.ttf') format('truetype'),
		url('fonts/Titillium_Web.woff2') format('woff2'),
		url('fonts/Titillium_Web.woff') format('woff')


